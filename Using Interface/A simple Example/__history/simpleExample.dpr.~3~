program simpleExample;

uses
  Vcl.Forms,
  people.view in 'people.view.pas' {Form1},
  people.interfaces in 'people.interfaces.pas',
  people.classes in 'people.classes.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TForm1, Form1);
  Application.Run;
end.
